{
  "name": "scandicraft_launcher",
  "version": "1.0.2",
  "description": "ScandiCraft Launcher",
  "main": "./dist/electron.bundle.js",
  "scripts": {
    "pack-dev": "npx webpack --config ./config/webpack/webpack.dev.js",
    "pack-prod": "npx webpack --config ./config/webpack/webpack.prod.js",
    "build-win64": "npm run pack-prod && electron-builder build --win --x64",
    "build-win32": "npm run pack-prod && electron-builder build --win --x32",
    "build-mac": "npm run pack-prod && electron-builder build --mac",
    "build-linux": "npm run pack-prod && electron-builder build --linux",
    "start": "electron ."
  },
  "devDependencies": {
    "@babel/core": "^7.9.6",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-react": "^7.9.4",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.5.3",
    "electron": "^8.3.0",
    "electron-builder": "^22.6.0",
    "electron-devtools-installer": "^3.0.0",
    "html-webpack-plugin": "^4.3.0",
    "style-loader": "^1.2.1",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.11"
  },
  "dependencies": {
    "@material-ui/core": "^4.9.14",
    "@material-ui/icons": "^4.9.1",
    "axios": "^0.19.2",
    "connected-react-router": "^6.8.0",
    "decompress-zip": "^0.2.2",
    "electron-log": "^4.1.2",
    "electron-store": "^5.1.1",
    "electron-updater": "^4.3.1",
    "esm": "^3.2.25",
    "history": "^4.10.1",
    "i": "^0.3.6",
    "jsonwebtoken": "^8.5.1",
    "npm": "^6.14.5",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-redux": "^7.2.0",
    "react-router-dom": "^5.2.0",
    "react-toastify": "^6.0.4",
    "redux": "^4.0.5",
    "redux-form": "^8.3.5",
    "redux-form-validators": "^3.3.2",
    "redux-thunk": "^2.3.0"
  },
  "engines": {
    "node": "12.x.x"
  },
  "build": {
    "productName": "ScandiCraft",
    "appId": "fr.scandicraft-mc",
    "files": [
      "dist/**/*",
      "public/splash.html"
    ],
    "mac": {
      "category": "public.app-category.games",
      "target": [
        "zip",
        "dmg"
      ]
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Game"
    },
    "nsis": {
      "perMachine": true
    },
    "directories": {
      "buildResources": "resources",
      "output": "build"
    },
    "publish": [
      {
        "provider": "generic",
        "url": "https://www.scandicraft-mc.fr/launcher/update"
      }
    ]
  },
  "author": {
    "name": "ScandiCraft",
    "email": "scandicraft.minecraft@gmail.com"
  },
  "private": true,
  "license": "UNLICENSED"
}
